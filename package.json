{
  "name": "@placemarkio/geojson-rewind",
  "version": "1.0.1",
  "description": "Fix the winding order of GeoJSON",
  "repository": "https://github.com/placemark/geojson-rewind.git",
  "author": "Tom MacWright",
  "license": "MIT",
  "main": "dist/rewind.cjs",
  "module": "dist/rewind.es.mjs",
  "browser": "dist/rewind.umd.js",
  "typings": "dist/index.d.ts",
  "exports": {
    "require": "./dist/rewind.cjs",
    "types": "./dist/index.d.ts",
    "default": "./dist/rewind.es.mjs"
  },
  "keywords": [
    "kml",
    "geojson"
  ],
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest run",
    "lint": "eslint . && tsc --noEmit",
    "doc": "typedoc --options typedoc.json",
    "prepare": "rollup -c rollup.config.mjs && npm run doc"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/geojson": "^7946.0.8",
    "eslint": "^8.18.0",
    "prettier": "^2.7.1",
    "rollup": "^3.2.4",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.19",
    "typescript": "^4.7.4",
    "vitest": "^0.24.4"
  },
  "dependencies": {
    "@changesets/cli": "^2.29.7"
  }
}
